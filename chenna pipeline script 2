   //Checkout Code stage
   stage('CheckOutCode'){
   steps{
      
   git credentialsId: '5f1f92e8-d0e0-4ec5-837a-af077e9fbed5', url: 'https://github.com/panduladdu/maven-web-application.git'
   }
   
 
   //Build the code
